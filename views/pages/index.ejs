<!DOCTYPE html>
<html>
	<!-- Start Head -->
	<head>
		<% include ../partials/head %>
	</head>
	<!-- End Head -->

	<!-- Start Body -->
	<body>

		<!-- Start Header -->
		<header>
			<% include ../partials/header %>
		</header>
		<!-- End Header -->

		<!-- Start Main Container -->
		<div class="container">
			<div class="wrap">
				<h2>Current Projects</h2>
				<ul>
			    <% projects.forEach(function(project) { %>
			        <li><%= project.name %> - <%= project.client %></li>
			    <% }); %>
				</ul>
			</div>
		</div>
		<!-- End Main Container -->

		<!-- Start Footer -->
		<footer>
			<% include ../partials/footer %>
		</footer>
		<!-- End Footer -->

	</body>
	<!-- End Body -->
</html>